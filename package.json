{
  "name": "node-sketch",
  "version": "0.2.0",
  "description": "Javascript library to manipulate sketch files",
  "main": "index.js",
  "author": "Oscar Otero",
  "license": "MIT",
  "dependencies": {
    "jsondiffpatch": "^0.2.4",
    "jszip": "^3.1.3"
  },
  "devDependencies": {
    "mocha": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarotero/node-sketch.git"
  },
  "keywords": [
    "sketch"
  ],
  "bugs": {
    "url": "https://github.com/oscarotero/node-sketch/issues"
  },
  "scripts": {
    "test": "npm run-script test:clean && npm run-script test:run && npm run-script test:diff1 && npm run-script test:diff2",
    "test:clean": "rm -rf ./tests/original && rm -rf ./tests/result",
    "test:run": "node ./tests/test.js && unzip ./tests/original.sketch -d ./tests/original && unzip ./tests/result.sketch -d ./tests/result",
    "test:diff1": "jsondiffpatch tests/original/pages/6624F3F7-470D-4370-9BDE-F81657777324.json tests/result/pages/6624F3F7-470D-4370-9BDE-F81657777324.json",
    "test:diff2": "jsondiffpatch tests/original/pages/A660427D-210F-4E17-AA74-DD9E72DA0E3B.json tests/result/pages/A660427D-210F-4E17-AA74-DD9E72DA0E3B.json"
  },
  "homepage": "https://github.com/oscarotero/node-sketch#readme"
}
